      "icon": "./build/favicon.png"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./build/favicon.png"
    },
    "nsis": {
      "oneClick": false
    },
    "directories": {
      "buildResources": "public",
      "output": "dist",
      "app": "."
    },
    "publish": {
      "provider": "github",
      "owner": "tsnullp",
      "repo": "sellerpublish",
      "token": "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"
    }
  },
